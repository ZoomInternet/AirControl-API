{
    "name": "ZoomNMS/aircontrol-api",
    "type": "library",
    "description": "Ubiquiti AirControl API client.",
    "keywords": ["aircontrol", "api", "airmax", "ubiquiti"],
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Dulin",
            "email": "dulinoz@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "^6.3",
        "psr/log": "^1.0"

    },
    "require-dev": {
        "phpspec/phpspec": "^3.0",
        "henrikbjorn/phpspec-code-coverage": "^3.0",
        "symfony/var-dumper": "^3.1",
        "squizlabs/php_codesniffer": "^3.0",
        "sebastian/phpcpd": "^2.0",
        "satooshi/php-coveralls": "dev-master",
        "symfony/console": "^3.1"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "AirControl\\Api\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
